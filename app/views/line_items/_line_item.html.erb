 <table id="entries">
				  <thead>
				    <tr>
				      <th>Name</th>
				      <th>Q'ty</th>
				      
				      <th colspan="3"></th>
				    </tr>
				  </thead>
 				 	
					  <% @cart.line_items.each do |line_item| %>
						<tr id="<%= dom_id(line_item) %>">
						  <td><%= truncate(strip_tags(line_item.product.name), :length=>15)%></td>
						  <td><%= line_item.quantity %></td>
						</tr>
						
					<% end %>
					
</table>
